{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/dist/",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/dist/"
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/dist/"
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validation/dist/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/dist/"
    },
    {
      "provider": "unpkg",
      "library": "nprogress@0.2.0",
      "destination": "wwwroot/lib/nprogress/dist/",
      "files": [
        "nprogress.css",
        "nprogress.js"
      ]
    }
  ]
